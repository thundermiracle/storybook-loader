{
  "presets": [
    "@babel/env"
  ],
  "plugins": [
    "add-module-exports",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": false,
        "regenerator": true,
      },
    ],
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "src": "./src",
          "core": "./src/core",
          "plugin": "./src/plugin"
        }
      }
    ]
  ],
  "ignore": ["node_modules/**"]
}