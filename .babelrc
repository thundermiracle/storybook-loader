{
  "presets": [
    "@babel/env",
    "@babel/react",
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "src": "./src",
          "core": "./src/core",
          "plugin": "./src/plugin",
          "test": "./test"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
         "add-module-exports",
      ],
    }
  },
  "ignore": ["node_modules/**"]
}