{
  "name": "storybook-loader",
  "version": "0.1.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thundermiracle/storybook-loader"
  },
  "author": "thundermiracle",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs:dev": "cd docs && yarn dev && cd ..",
    "docs:build": "cd docs && yarn && yarn build && cd ..",
    "build:rm": "rm -rf dist/",
    "build": "yarn build:rm && cross-env NODE_ENV=production babel src --out-dir ./dist --verbose",
    "eslint": "eslint ./src && eslint ./test",
    "jest": "jest",
    "test": "yarn eslint && yarn jest && codecov"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.7",
    "@storybook/react": "^5.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-react-require": "^3.1.1",
    "codecov": "^3.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.8.3",
    "storybook-readme": "^4.0.5"
  }
}
