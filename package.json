{
  "name": "storybook-loader",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint ./src",
    "build:rm": "rm -rf dist/",
    "build:cp": "node ./scripts/copyFiles dist/ package.json README.md CHANGELOG.md LICENSE",
    "build": "npm run build:rm && cross-env NODE_ENV=production babel src --out-dir ./dist --verbose && npm run build:cp",
    "jest": "jest",
    "test": "npm run eslint && npm run jest"
  },
  "peerDependencies": {
    "@storybook/react": "^4.0.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "loglevel": "^1.6.1",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.7",
    "@storybook/react": "^4.1.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "storybook-readme": "^4.0.5"
  },
  "jest": {
    "transform": {}
  }
}
